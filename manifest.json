{
  "name": "TOOO",
  "description": "Translators of Our Own",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://archiveofourown.org/works/new",
                  "*://www.archiveofourown.org/works/new",
                  "*://archiveofourown.org/works/*/chapters/new",
                  "*://www.archiveofourown.org/works/*/chapters/new"],
      "js": ["jquery.min.js", "post_new.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [ "*/messages.json" ],
      "matches": ["<all_urls>"]
    }
  ],
  "default_locale": "zh_TW"
}
